{
  "name": "osu-mgr-website",
  "author": "@osu-mgr",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "set NODE_OPTIONS=--inspect&& next dev -p 3005",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@mdx-js/runtime": "^1.6.22",
    "markdown-to-jsx": "^7.1.1",
    "next": "10.0.6",
    "next-images": "^1.7.0",
    "next-tinacms-github": "^0.36.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "11.3.0",
    "react-tinacms-editor": "^0.36.1",
    "react-tinacms-github": "^0.36.1",
    "react-tinacms-inline": "^0.36.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "styled-components": "^5.2.1",
    "tinacms": "^0.36.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.24",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
